extends ../layout

block content
  .span3
    nav.well.sidebar-nav
      ul.nav.nav-list
        li
          a(href="/person/get")
            i.icon-plus
            = 'Create new'
        li.nav-header Created
  .span5
    table
      each like in likes
        tr
          td= like._id
          td= like.value.count
          td
            each share in like.value.sharedBy
              = share + ', '
